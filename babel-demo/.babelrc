{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",   // babel-polyfill 按需引入
                "corejs": 3 // corejs 的版本号
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": 3,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ]
}